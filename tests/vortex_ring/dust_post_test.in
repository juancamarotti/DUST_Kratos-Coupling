basename = Postpro/part
data_basename = Output/part

analysis = {
  name = v1
  type = Viz

  format = vtk !tecplot

  start_res = 1
  end_res = 9999
  step_res = 5 
  wake = T

  variable = vorticity_vector
}

analysis = {
  name = flow
  type = flow_field 

  format = vtk !tecplot

  start_res = 1
  end_res = 9999
  step_res = 5 
  wake = T

  n_xyz = (/ 1, 300, 700/)
  min_xyz = (/ 0.0, -1.5, -0.5 /)
  max_xyz = (/ 0.0, 1.5, 6.5 /)

  variable = Velocity
}

